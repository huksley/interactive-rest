{
  "name": "testresti",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "build": "rm -Rf .build; NODE_ENV=production node src/build",
    "start": "NODE_ENV=production node src/server",
    "predist": "npm run build",
    "dist": "rm -Rf dist; mkdir -p dist; cp -R static/* dist; cp -R src/pages/* dist/; cp -R .build/src/pages/* dist/",
    "postdist": "find . ! -path \"./node_modules/*\" ! -path \"./.git/*\"",
    "postvercel": "if [ \"$VERCEL\" = \"1\" ]; then rm -Rf api/*; cp -R .build/src/api/* api/; fi",
    "vercel": "npm run dist",
    "vercel-build": "echo \"Invoked before packing functions\"",
    "dev": "nodemon"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.12",
    "js-md5": "^0.7.3",
    "mime-types": "^2.1.35",
    "nanoid": "^4.0.0",
    "pusher": "^5.1.1-beta",
    "pusher-js": "^7.4.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.4.2",
    "simpledotcss": "^2.1.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.10",
    "fs-monkey": "^1.0.3",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.18",
    "postcss-import": "^15.0.0",
    "postcss-load-config": "^4.0.1",
    "tailwindcss": "^3.1.8"
  }
}
